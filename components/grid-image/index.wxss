.title {
  font-weight: 500;
  font-size: 12px;
}
.describe {
  font-size: 10px;
}
.like {
  font-size: 10px;
}
.no-image {
  width: 90%;
  height: 150px;
  border-radius: 8px;
  background-color: rgb(245, 235, 226);
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.error-text {
  margin-top: 10rpx;
}
.status-view {
  position: absolute;
  left: 35%;
  top: 35%;
  border-style: dashed;
  border-width: 3rpx;
  margin: 5rpx;
}
.status-text {
  color: #FF6700;
  font-weight: 500;
}
.t-progress {
  position: absolute;
  left: 15%;
  top: 15%;
